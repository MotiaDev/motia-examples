{
  "name": "rag-docling-vector-agent",
  "version": "1.0.0",
  "description": "RAG Agent to process and vectorize PDF documents using Motia, Docling with support for Weaviate and ChromaDB",
  "main": "index.js",
  "scripts": {
    "postinstall": "motia install",
    "prepare": "python3 -m venv python_modules && bash -c 'source python_modules/bin/activate && pip install -r requirements.txt'",
    "dev": "bash -c 'source python_modules/bin/activate && motia dev'",
    "dev:debug": "bash -c 'source python_modules/bin/activate && motia dev --verbose'",
    "start": "bash -c 'source python_modules/bin/activate && motia start'",
    "generate-types": "motia generate-types",
    "build": "bash -c 'source python_modules/bin/activate && motia build'",
    "clean": "rm -rf dist .motia .mermaid node_modules python_modules",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "test": "jest --coverage"
  },
  "keywords": [
    "motia",
    "rag",
    "pdf",
    "weaviate",
    "chromadb"
  ],
  "author": "Filipe Leandro <github:ffleandro>",
  "license": "MIT",
  "dependencies": {
    "chromadb": "^1.9.2",
    "cohere-ai": "^7.14.0",
    "dotenv": "^16.5.0",
    "motia": "^0.5.5-beta.113",
    "ollama": "^0.5.9",
    "openai": "^4.104.0",
    "weaviate-client": "^3.4.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.28",
    "@types/react": "^18.3.23",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  }
}
